<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <attr format="color" name="bt_expiration_date_dialog_divider"/>
    <attr format="color" name="bt_expiration_date_dialog_label"/>
    <attr format="color" name="bt_expiration_date_scrollbar_thumb_vertical"/>
    <attr format="color" name="bt_expiration_date_scrollbar_track_vertical"/>
    <attr format="color" name="bt_expiration_date_sheet_background"/>
    <color name="bt_black">#001129</color>
    <color name="bt_black_12">#1e000000</color>
    <color name="bt_black_38">#61000000</color>
    <color name="bt_black_54">#8a000000</color>
    <color name="bt_black_87">#de000000</color>
    <color name="bt_blue">#009bde</color>
    <color name="bt_expiration_date_sheet_background_dark">#20272b</color>
    <color name="bt_expiration_date_sheet_background_light">#eceff0</color>
    <color name="bt_light_gray">#6d6d6d</color>
    <color name="bt_red">#dd5555</color>
    <color name="bt_white">#ffffff</color>
    <color name="bt_white_12">#1effffff</color>
    <color name="bt_white_38">#61ffffff</color>
    <color name="bt_white_54">#8affffff</color>
    <color name="bt_white_87">#deffffff</color>
    <dimen name="bt_expiration_date_item_selected_background_radius">2dp</dimen>
    <dimen name="bt_expiration_date_sheet_height">260dp</dimen>
    <dimen name="bt_expiration_date_sheet_label_padding_bottom">8dp</dimen>
    <dimen name="bt_expiration_date_sheet_label_padding_top">16dp</dimen>
    <dimen name="bt_expiration_date_sheet_left_right_margin">16dp</dimen>
    <dimen name="bt_expiration_date_sheet_top_bottom_margin">16dp</dimen>
    <dimen name="bt_floating_edit_text_horizontal_offset">12dp</dimen>
    <dimen name="bt_full_width" ns1:ignore="ReferenceType">@integer/bt_match_parent</dimen>
    <dimen name="bt_input_text_size">16sp</dimen>
    <dimen name="bt_landscape_max_width">500dp</dimen>
    <integer name="bt_match_parent">-1</integer>
    <string name="bt_default_action_label">Purchase</string>
    <string name="bt_form_hint_card_number">Card Number</string>
    <string name="bt_form_hint_cvv">CVV</string>
    <string name="bt_form_hint_expiration">Expiration</string>
    <string name="bt_form_hint_postal_code">Postal Code</string>
    <string name="bt_month" ns1:ignore="MissingTranslation">Month</string>
    <string name="bt_year" ns1:ignore="MissingTranslation">Year</string>
    <style name="bt_base_textview">
        <item name="android:textSize">@dimen/bt_input_text_size</item>
    </style>
    <style name="bt_card_form_field" parent="bt_base_textview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:textColorHint">@color/bt_light_gray</item>
        <item name="android:textCursorDrawable" ns1:ignore="NewApi">@null</item>
        <item name="android:background">@null</item>
    </style>
    <style name="bt_card_form_field_landscape" parent="bt_card_form_field">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="bt_expiration_date_dialog_dark" parent="bt_expiration_date_dialog_sheet">
        <item name="bt_expiration_date_sheet_background">@color/bt_expiration_date_sheet_background_dark</item>
        <item name="bt_expiration_date_dialog_label">@color/bt_white_54</item>
        <item name="bt_expiration_date_dialog_divider">@color/bt_white_12</item>
        <item name="bt_expiration_date_scrollbar_thumb_vertical">@color/bt_white_54</item>
        <item name="bt_expiration_date_scrollbar_track_vertical">@color/bt_white_12</item>
    </style>
    <style name="bt_expiration_date_dialog_grid_view">
        <item name="android:layout_marginLeft">@dimen/bt_expiration_date_sheet_left_right_margin</item>
        <item name="android:layout_marginRight">@dimen/bt_expiration_date_sheet_left_right_margin</item>
        <item name="android:layout_marginBottom">@dimen/bt_expiration_date_sheet_top_bottom_margin</item>
        <item name="android:listSelector">@null</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:numColumns">auto_fit</item>
        <item name="android:columnWidth">72dp</item>
    </style>
    <style name="bt_expiration_date_dialog_label">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">@dimen/bt_expiration_date_sheet_label_padding_top</item>
        <item name="android:paddingBottom">@dimen/bt_expiration_date_sheet_label_padding_bottom</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/bt_expiration_date_dialog_label</item>
    </style>
    <style name="bt_expiration_date_dialog_light" parent="bt_expiration_date_dialog_sheet">
        <item name="bt_expiration_date_sheet_background">@color/bt_expiration_date_sheet_background_light</item>
        <item name="bt_expiration_date_dialog_label">@color/bt_black_54</item>
        <item name="bt_expiration_date_dialog_divider">@color/bt_black_12</item>
        <item name="bt_expiration_date_scrollbar_thumb_vertical">@color/bt_black_54</item>
        <item name="bt_expiration_date_scrollbar_track_vertical">@color/bt_black_12</item>
    </style>
    <style name="bt_expiration_date_dialog_sheet" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowAnimationStyle">@style/bt_expiration_date_dialog_sheet_animation</item>
    </style>
    <style name="bt_expiration_date_dialog_sheet_animation">
        <item name="android:windowEnterAnimation">@anim/bt_expiration_date_dialog_open</item>
        <item name="android:windowExitAnimation">@anim/bt_expiration_date_dialog_close</item>
    </style>
    <style name="bt_expiration_date_dialog_year_grid_view" parent="bt_expiration_date_dialog_grid_view">
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingEnd" ns1:targetApi="jelly_bean_mr1">8dp</item>
        <item name="android:scrollbarSize">3dp</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:fadeScrollbars">false</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
    </style>
</resources>